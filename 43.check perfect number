using System;
class Program
{
    static void Main()
    {
        Console.Write("Enter a number: ");
        int num = int.Parse(Console.ReadLine());
        int sum = 0;

        for (int i = 1; i < num; i++)
            if (num % i == 0) sum += i;

        Console.WriteLine(sum == num ? "Perfect Number" : "Not a Perfect Number");
    }
}
