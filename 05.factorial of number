using System;

class Program
{
    static void Main()
    {
        int num, factorial = 1;

        Console.Write("Enter a number: ");
        num = Convert.ToInt32(Console.ReadLine());

        for (int i = 1; i <= num; i++)
        {
            factorial *= i;
        }

        Console.WriteLine("Factorial of " + num + " is: " + factorial);
    }
}
