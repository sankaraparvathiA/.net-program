using System;

class Program
{
    static void Main()
    {
        int num;
        bool isPrime = true;

        Console.Write("Enter a number: ");
        num = Convert.ToInt32(Console.ReadLine());

        for (int i = 2; i <= num / 2; i++)
        {
            if (num % i == 0)
            {
                isPrime = false;
                break;
            }
        }

        if (isPrime && num > 1)
            Console.WriteLine(num + " is a Prime number.");
        else
            Console.WriteLine(num + " is not a Prime number.");
    }
}
