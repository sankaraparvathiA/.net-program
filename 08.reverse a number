using System;
class Program
{
    static void Main()
    {
        int num, reversed = 0;
        Console.WriteLine("Enter a number:");
        num = Convert.ToInt32(Console.ReadLine());
        while (num != 0)
        {
            reversed = reversed * 10 + num % 10;
            num /= 10;
        }
        Console.WriteLine("Reversed number: " + reversed);
    }
}
